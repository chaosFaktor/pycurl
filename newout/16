    $(document).ready(function() {

    $('.footerIMG').hover(
	  	function() {
	    	var src = $(this).attr("src");
	    	var newSRC = src.replace('-bw.png', '.png');
	    	$(this).attr("src", newSRC);

	  	}, function() {
	    	var src = $(this).attr("src");
