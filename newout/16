  <script type="text/javascript">
   $(document).ready(function() {

	    $('.fancybox').fancybox({
		    type: 'iframe',
		    autoSize: false,
		    scrolling : 'no',
		    autoSize : false,
	        
	        beforeLoad : function() {         
	            this.width  = parseInt(this.element.data('fancybox-width'));  
	            this.height = parseInt(this.element.data('fancybox-height'));
	        }
		});

		$('.searchinput').autocomplete( {   
	        source: "https://www.druckspezialist.eu/ajax/search-autocomplete.php",
	        delay: 300,
	        minLength: 2,
	        position: {
     		   my : "right top",
		       at: "right bottom"
    		},
	        select: function() {
	        	$("#search").submit();
  			}
  		}).focus(function() { 
  			$(this).find('input').select(); 
  			$(this).select(); 
  		});

  		if($(window).width() > 760) {

			$("#navBarShop").affix({
	            offset: { 
	            	top: function () {
				      return ($('#header').outerHeight(true))
				    },
				    bottom: function () {
				      return ($('#footerTotal').outerHeight(true))
				    }
	            }
	        });
		}
	});
  </script>
