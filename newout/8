  dataLayer.push({
			'event': 'view_item',
			'ecommerce': {
				'items': [{
	      			'item_name': 'Metall Kugelschreiber KING Werbekugelschreiber mit Gravur Text Name', // Name or ID is required.
		      		'item_id': 'ArtID_8',
	    	  		'price': 2.99,
	      			'quantity': 1
	      		}]
